<table class="table table-striped col-md-8 justify-content-center ">
    <thead>
        <tr>
            <th scope="col">Branch ID</th>
            <th scope="col">Name</th>
            <th scope="col">Address</th>
            <th scope="col">Contact Number</th>
        </tr>
    </thead>
    <tbody>
        {{#each trendingBranches }}
        <tr>
            <td>{{ this.id }}</td>
            <td>{{ this.bname }}</td>
            <td>{{ this.baddress }}</td>
            <td>{{ this.bphone }}</td>
        </tr>
        {{/each}}
    </tbody>
</table>