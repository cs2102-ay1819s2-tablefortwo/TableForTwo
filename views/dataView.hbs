<h1>CS2102: {{ title }}</h1>
	<table class="table">
		<thead>
			<tr>
				<th scope="col">Matric</th>
				<th scope="col">Name</th>
				<th scope="col">Faculty</th>
			</tr>
		</thead>
		<tbody>
			{{#each data}}
			<tr>
				<td>{{ this.matric }}</td>
				<td>{{ this.name }}</td>
				<td>{{ this.faculty }}</td>
			</tr>
			{{/each}}
		</tbody>
	</table>
